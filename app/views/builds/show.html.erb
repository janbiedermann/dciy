<div class="page-header">
  <div class="page-actions">
    <%= form_for(@new_build) do |f| %>
      <%= f.hidden_field :project_id %>
      <%= f.hidden_field :branch %>
      <%= f.submit "Punch it again!", class: "button button-primary" %>
    <% end %>
  </div>
  <div class="page-title">
    <h1><span><%= @build.project.repo %></span> @ <%= @build.branch %></h1>
  </div>
</div>

<dl>
  <dt>Branch</dt>
  <dd><%= @build.branch %></dd>


  <dt>SHA</dt>
  <dd>
    <span class="js-build-sha">
      <%= @build.sha || "HEAD" %>
    </span>
  </dd>

  <dt>Status</dt>
  <dd>
    <span class="js-build-status" data-status="<%= @build.successful %>">
      <%= @build.status_phrase %>
    </span>
  </dd>
</dl>





<pre class="build-output js-build-output" data-uri="<%= build_path(@build, :format => :json) %>">
<%= @build.output %>
</pre>
